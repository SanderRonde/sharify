		"express-ws": "^4.0.0",
		"fs-extra": "^8.1.0",
		"human-readable-ids": "^1.0.4",
		"morgan": "^1.9.1",
		"node-fetch": "^2.6.1",
		"qrcode": "^1.4.4",
		"serve-static": "^1.14.1"
	},
	"devDependencies": {
		"@types/cookie-parser": "^1.4.2",
		"@types/express": "^4.17.2",
		"@types/express-ws": "^3.0.0",
		"@types/fs-extra": "^8.1.0",
		"@types/morgan": "^1.7.37",
		"@types/node-fetch": "^2.5.4",
		"@types/qrcode": "^1.3.4",
		"nodemon": "^2.0.2",
		"typescript": "^3.8.2"
